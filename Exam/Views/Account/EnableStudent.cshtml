@using Exam.Models.ViewModel
@model RegistrationStudentViewModel
@{
    ViewData["Title"] = "EnableStudent";
}

<h1>EnableStudent</h1>

<form asp-action="EnableStudent" method="post">
    <table class="table table-bordered">
        <tr>
            <td><label asp-for="City"></label></td>
            <td>
                <input asp-for="City" class="form-control" />
                <span asp-validation-for="City" class="text-danger"></span>
            </td>
        </tr>
        <tr>
            <td colspan="2">
                <input type="submit" class="btn btn-success" value="Enable As Student" />
            </td>
        </tr>

    </table>


    <div asp-validation-summary="All" class="text-danger"></div>


</form>